pipeline {
   agent any
    stages {
       stage('Git checkout') {
          steps {
               script{
                     dir("AnsibleD")
                     {
                       git branch: 'main', url: 'https://github.com/boopathygit/Ansible.git'
                     }
               } 
          }
       }
       stage('Execute Ansible Playbook') {
          steps {
             sh  "ansible-playbook User_Create.yml"
          }
       } 
    }
}
